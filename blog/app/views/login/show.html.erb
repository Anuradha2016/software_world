<p>
  <strong>Tittle:</strong>
  <%= @login.tittle %>
</p>
 
<p>
  <strong>Text:</strong>
  <%= @login.text %>
</p>
 

<%= form_for :login, @login,  do |f| %>
  <p>
    <%= f.label :tittle %><br>
    <%= f.text_field :tittle %>
  </p>
  <p>
    <%= f.label :text%><br>
    <%= f.text_area :text%>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
 <p><%= link_to 'show', :action => "index"%></p>
 
